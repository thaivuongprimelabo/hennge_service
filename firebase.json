{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
        "source": "/api/sendmail",
        "function": "sendmail"
      },
      {
        "source": "**",
        "destination": "/index.html"
      } 
    ]
  }
}
